(fn show-name-with-age [name age]
    (do
        (if (= name "John")
            (print (str "Oh, Hello, John"))
            (print (str "Hello, " name ".")))
        (print (str "\nYou are "  age " years old."))
        (print "\n\r")
    ))

(define josh "Josh")
(define josh-age 22)

(define john "John")
(define john-age 24)

(show-name-with-age josh josh-age)
(show-name-with-age john john-age)
